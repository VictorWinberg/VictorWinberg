{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["🚀", "🌟", "🔖"]
    },
    {
      "title": "## 🐛 Fixes",
      "labels": ["🐛", "🚨"]
    },
    {
      "title": "## 🧪 Tests",
      "labels": ["🧪"]
    }
  ],
  "pr_template": "${{TITLE}}",
  "label_extractor": [
    {
      "pattern": "(.) (.+)",
      "target": "$1"
    }
  ],
  "transformers": [
    {
      "pattern": "(.) (.+)",
      "target": "- $2"
    }
  ],
  "max_pull_requests": 1000,
  "max_back_track_time_days": 1000
}
